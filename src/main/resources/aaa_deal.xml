<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<configuration>

        <property>
                 <name>aaa.data.separator</name>
                 <value>\u007c</value>
                 <description>数据分隔符</description>
        </property>
        
        <property>
                 <name>aaa.data.decrypt.fields</name>
                 <value>0,4,5</value>
                 <description>需要解密的字段，以逗号为分隔符</description>
        </property>
        
        <property>
                 <name>aaa.data.decrypt.class</name>
                 <value>com.bonc.functions.aaa.decrypt.DefaultDecrypt</value>
                 <description>解密类</description>
        </property>
        
</configuration>